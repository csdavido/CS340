<h2>Class Enrollment List</h2>
<table>
  <thead>
    <th>Enrollment ID</th>
    <th>Student ID</th>
    <th>Class ID</th>
  </thead>
  <tbody>
  {{#each class_enrollment}}
  <tr>
    <td>{{id}}</td>
    <td>{{student_id}}</td>
    <td>{{class_code}}</td>
    <td><button onclick="deleteStudentFromClass({{id}})">Delete</button></td>
  </tr>
  {{/each}}
  </tbody>
</table>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>