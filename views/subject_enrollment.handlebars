<h2>Subject Enrollment List</h2>
<table>
  <thead>
    <th>ID</th>
    <th>Student ID</th>
    <th>Subject Code</th>
  </thead>
  <tbody>
  {{#each subject_enrollment}}
  <tr>
    <td>{{id}}</td>
    <td>{{student_id}}</td>
    <td>{{subject_code}}</td>
    <td><button onclick="deleteStudentFromSubject({{id}})">Delete</button></td>
  </tr>
  {{/each}}
  </tbody>
</table>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>